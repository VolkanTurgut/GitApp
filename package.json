{
  "name": "GitApp",
  "version": "2.4.9",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "release": "sudo rm -rf dist; sudo build -mwl -p always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-online/GitApp"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "dan-online",
        "repo": "GitApp",
        "publishAutoUpdate": true,
        "releaseType": "prerelease"
      }
    ],
    "artifactName": "${productName}-${os}.${ext}",
    "appId": "com.github.dan-online.GitApp"
  },
  "keywords": [
    "Electron app",
    "Github Web",
    "Github App"
  ],
  "author": "DanCodes",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.2"
  },
  "dependencies": {
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6"
  }
}
