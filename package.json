{
  "name": "Github-Web",
  "version": "2.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "publish": "build -m -p always"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/dan-online/Github-WebApp"
  },

  "build": {
    "publish": [{
        "provider": "github",
        "owner": "dan-online",
        "repo": "Github-WebApp"
    }],
    "appId": "com.github.dan-online.GithubWebApp"
  },
  "win": {
    "publish": ["github", "bintray"]
  },
  "mac": {
    "publish": ["github", "bintray"]
  },
  "keywords": [
    "Electron app",
    "Github Web",
    "Github App"
  ],
  "author": "DanCodes",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.2"
  },
  "dependencies": {
    "electron-updater": "^4.0.6"
  }
}
